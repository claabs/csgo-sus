{
  "name": "csgo-sus",
  "version": "0.1.0",
  "description": "Lookup in-depth public data on CSGO players' accounts to see if they're suspicious",
  "scripts": {
    "test": "jest --coverage",
    "build": "rimraf dist && tsc",
    "lint": "tsc --noEmit && eslint src/**/*.ts",
    "start": "ts-node src/index.ts",
    "docker:build": "docker build . -t ghcr.io/claabs/csgo-sus:latest --target deploy",
    "docker:run": "mkdir -p volume && chmod 777 volume && docker run --rm -ti --security-opt seccomp=\"https://raw.githubusercontent.com/ulixee/secret-agent/main/tools/docker/seccomp_profile.json\" -v $(pwd)/volume:/csgo-sus --env-file .env ghcr.io/claabs/csgo-sus:latest"
  },
  "keywords": [
    "csgostats.gg",
    "csgo",
    "opponent",
    "player",
    "faceitfinder"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/claabs/csgo-sus.git"
  },
  "homepage": "https://github.com/claabs/csgo-sus",
  "author": "claabs",
  "license": "AGPL-3.0-only",
  "dependencies": {
    "@discordjs/builders": "^0.8.2",
    "@discordjs/rest": "^0.1.1-canary.0",
    "axios": "^0.24.0",
    "csgostatsgg-scraper": "^1.1.1",
    "discord-api-types": "^0.24.0",
    "discord.js": "^13.3.1",
    "dotenv": "^10.0.0",
    "fs-extra": "^10.0.0",
    "hybrid-disk-cache": "^0.2.0",
    "moment": "^2.29.1",
    "pino": "^7.1.0",
    "pino-pretty": "^7.2.0",
    "source-map-support": "^0.5.19",
    "steamapi": "^2.2.0",
    "steamcommunity-inventory": "^2.0.5",
    "steamid": "^2.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.10",
    "@types/steamapi": "^2.2.1",
    "@types/steamid": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^25.2.2",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.1",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.4"
  },
  "engines": {
    "node": "16"
  }
}
